{
  "name": "navtree",
  "version": "1.0.0",
  "description": "A lightweight navigation tree based on webpack and babel.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Test: Booting up Jest test cases!\" &&  jest",
    "build": "echo \"Build: Building production env!\" && webpack -p --progress",
    "start": "echo \"Start: Booting up web server and watch!\" && webpack-dev-server --output-public-path=/dist/js"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "no-mixed-spaces-and-tabs": [
        0,
        "smart-tabs"
      ],
      "no-console": 1
    }
  },
  "author": "Freedompay, Inc.",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.5.1",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "infinite-tree": "^1.16.2"
  }
}
